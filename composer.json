{
  "name": "thiagomarini/not-tomorrow",
  "version": "1.0.1",
  "description": "PHP library to calculate the next working day. You can also check if a given date is a holiday or if the next day is on weekend.",
  "keywords": [
    "find",
    "next",
    "working",
    "day",
    "holiday",
    "weekend"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thiago Marini"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "NotTomorrow\\": "src/NotTomorrow/"
    }
  }
}
